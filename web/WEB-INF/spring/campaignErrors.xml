<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                           http://www.springframework.org/schema/util http://www.springframework.org/schema/util/spring-util-2.5.xsd">

  <bean id="campaignNotFoundError" class="edu.ucla.cens.awserver.validator.json.FailedJsonRequestAnnotator">
    <constructor-arg index="0">
      <bean class="edu.ucla.cens.awserver.domain.ErrorResponse">
        <property name="code" value="0808" />
        <property name="text" value="Campaign not found." />
      </bean>
    </constructor-arg>
  </bean>
  
</beans>